.card {
  background-color: #f2f2f2;
  width: 350px;
  height: 75px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-left: 10px;
  margin-right: 10px;
  margin-bottom: 10px;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  -webkit-box-shadow: 0 0 5px 1px rgba(0, 0, 0, 0.1);
          box-shadow: 0 0 5px 1px rgba(0, 0, 0, 0.1);
}
.card .company-info {
  width: 50px;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.card .content-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 100%;
}
.card .content-info .airport-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
.card .content-info .airport-info .from-info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20px;
  font-size: 20px;
  color: #333;
}
.card .content-info .airport-info .from-info .from-place {
  color: #999;
  font-size: 16px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.card .content-info .airport-info .time-info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20px;
}
.card .content-info .airport-info .time-info .howlong {
  font-size: 14px;
  text-align: center;
}
.card .content-info .airport-info .time-info .jiantou {
  border-bottom: 1px solid black;
  height: 7px;
  width: 90%;
}
.card .content-info .airport-info .time-info .jiantou .arrowRight:after {
  content: "";
  width: 6px;
  height: 7px;
  display: inline-block;
  -webkit-transform-origin: bottom right;
          transform-origin: bottom right;
  border-right: 1px solid black;
  -webkit-transform: rotate(-60deg);
          transform: rotate(-60deg);
  float: right;
}
.card .content-info .airport-info .to-info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20px;
  font-size: 20px;
  color: #333;
}
.card .content-info .airport-info .to-info .to-place {
  color: #999;
  font-size: 16px;
  display: -webkit-box;
  /*将对象转为弹性盒模型展示*/
  -webkit-box-orient: vertical;
  /*设置弹性盒模型子元素的排列方式*/
  -webkit-line-clamp: 1;
  /*限制文本行数*/
  overflow: hidden;
  /*超出隐藏*/
}
.card .content-info .plane-info {
  color: #999;
  font-size: 14px;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
